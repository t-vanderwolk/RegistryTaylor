{
  "name": "registry-taylor",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "cd backend && npm start",
    "dev": "concurrently \"npm run dev --prefix backend\" \"npm start --prefix frontend\"",
    "migrate": "cd backend && npm run migrate",
    "seed": "cd backend && npm run seed",
    "heroku-postbuild": "cd frontend && npm install && npm run build && cd ../backend && npm install && npm run migrate && npm run seed"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "pg": "^8.11.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^3.0.3"
  }
}